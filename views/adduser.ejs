      <div class="main-panel">
        <div class="content-wrapper">


            <div class="col-12 grid-margin stretch-card">
                <div class="card">
                  <%- include ('./partials/messages') %>
                  <div class="card-body">
                    <h4 class="card-title">Add System User</h4>
                    <p class="card-description">
                     System User
                    </p>
                    <form class="forms-sample" action="/adduser" method="post">
                      <div class="form-group">
                        <label for="exampleInputName1">Name</label>
                        <input type="text" class="form-control" id="username" name="username" placeholder="Name">
                      </div>
                      <div class="form-group">
                        <label for="exampleInputEmail3">Email address</label>
                        <input type="email" class="form-control" id="email" name="email" placeholder="Email">
                      </div>
                      <div class="form-group">
                        <label for="exampleInputPassword4">Password</label>
                        <input type="password" class="form-control" id="password" name="password" placeholder="Password">
                      </div>
                      <div class="form-group">
                        <label for="exampleInputPassword4">Re-Type Password</label>
                        <input type="password" class="form-control" id="repassword" name="repassword" placeholder="Password">
                      </div>
                     
                      <div class="form-group">
                        <label for="exampleInputEmail3">User Roll</label>
                      <select class="form-control" name="userroll" id="userroll">
                        <option value="0">Please Select User Roll</option>
                        <option value="Admin">Admin</option>
                        <option value="Inventory_Manager">Inventory Manager</option>
                      </select>
                      
                      </div>
                      <div class="col-md-6 offset-md-3">
                        <button type="submit" class="btn btn-primary btn-block">Save</button>
                      </div>
                    </form>
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-md-12">
               
                  <div class="card">
                 
                   <div class="card-body">
                    <h5 class="card-title">Customer List</h5>
                    <p class="card-description">
                     <code>LIst Of Customer</code>
                    </p>
                    <div class="table-responsive pt-3">
                      <table class="table table-striped project-orders-table" >
                        <thead>
                          <tr>
                            <th class="ml-5">#</th>
                            <th>User Name</th>
                            <th>Email</th>
                            <th>Is Active</th>
                            <th>Userroll</th>
                            <th>Action </th>
                            <th> ToDo</th>
                          </tr>
                        </thead>
                        <tbody>
                          <% if (account.length >0){ %>
                            <% var i =0; %> 
                         
                          <% account.forEach(function(row){ %>
                            <% i++ ; %>
                         <tr>
                          <td><%= i %></td>
                           <td><%= row.username %></td>
                           <td><%= row.email %></td>
                           <td><%= row.isactive %></td>
                           <td><%= row.userroll %></td>
                           <td><form action="/edituser/<%= row.userid %>" method="get">
                            <button type="submit" class="btn btn-block btn-primary">Edit<i class=" fa fa-edit"></i></button>
                          </form></td>
                           <td><form action="/deleteuser/<%= row.userid %>" method="get">
                            <button type="submit" class="btn btn-block btn-primary">Delete<i class=" fa fa-trash"></i></button>
                           </form></td>
                        
                          </tr>
                       
                          <% })} else { %>
                      
                            <%  } %>
                        </tbody>
                      </table>
                    </div>
                   </div>
                <div class="card-footer">
                  
                </div>
                  </div>
                </div>
              </div>
        </div>
